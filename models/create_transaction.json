{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "transactions",
    "type": "object",
    "properties": {
      "amount": {
        "type": "number",
        "minimum": 1
      },
      "category": {
        "type": "string",
        "enum": ["credit", "debit"]
      },
      "title": {
        "type": "string"
      }
    },
    "required": ["amount", "category", "title"],
    "additionalProperties": false
}